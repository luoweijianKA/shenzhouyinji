<view class="list">
    <t-tabs defaultValue="Used" bind:change="onTabsChange" sticky="true">
        <t-tab-panel label="已使用（{{pageInfo.useCount}}）" value="Used">
            <view class="item" wx:for="{{list}}" wx:key="id">
                <coupon-item bindqrcode="handleQrcode" data-item="{{item}}" item="{{item}}" />
            </view>
        </t-tab-panel>
        <t-tab-panel label="已过期（{{pageInfo.expireCount}}）" value="Expired">
            <view class="item" wx:for="{{list}}" wx:key="id">
                <coupon-item bindqrcode="handleQrcode" data-item="{{item}}" item="{{item}}" />
            </view>
        </t-tab-panel>
    </t-tabs>
</view>
<qrcode-popup visible="{{visible}}" qrcode="{{qrcode}}" tideSpotName="{{tideSpotName}}" />