<view class="list">
    <t-tabs defaultValue="used" bind:change="onTabsChange" bind:click="onTabsClick" bottom-line-mode="auto">
        <t-tab-panel label="已使用（4）" value="used">
            <view class="item">
                <coupon-item bindqrcode="handleQrcode" data-item="" />
            </view>
        </t-tab-panel>
        <t-tab-panel label="已过期（4）" value="expired">
            <view class="item">
                <coupon-item bindqrcode="handleQrcode" data-item="" />
            </view>
        </t-tab-panel>
    </t-tabs>
</view>
<qrcode-popup visible="{{visible}}" qrcode="{{qrcode}}" />