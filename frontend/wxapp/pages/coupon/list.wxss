page {
  background-color: #f5f5f5;
  padding-bottom: 132rpx;
}
.item {
  margin: 32rpx 22rpx 0;
}
.list .t-tabs {
  background-color: #f5f5f5;
}
.footer {
  height: 132rpx;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  text-align: center;
  line-height: 132rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 60rpx;
}
.button {
  flex: 1;
  margin-right: 20rpx;
}
.button:last-child {
  margin-right: 0;
}
