page {
  background-color: #f5f5f5;
}
.wrapper {
  margin: 32rpx 22rpx;
  padding: 34rpx 26rpx;
  border-radius: 20rpx;
  background-color: #fff;
}
.video {
  width: 654rpx;
  height: 368rpx;
}
.rule {
  padding-top: 50rpx;
}
.title {
  padding-bottom: 38rpx;
  font-size: 32rpx;
  font-weight: 600;
  line-height: 28rpx;
  color: #333;
}
.footer {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 132rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
  box-shadow: 0 8rpx 20rpx 0 rgba(0, 0, 0, 0.3);
}
.button {
  width: 240rpx;
  height: 80rpx;
}
