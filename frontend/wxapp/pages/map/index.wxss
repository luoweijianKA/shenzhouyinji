.map-container {
  width: 100%;
  height: 100vh;
}

.external-class {
  border: none;
  border-radius: 0;
}

.location {
  margin: 24rpx;
  margin-bottom: 0;
}

.location>.img {
  width: 56rpx;
  height: 56rpx;
}

.fd {
  position: fixed;
  z-index: 999;
  bottom: 160rpx;
  width: 100%;
  flex-direction: row;
}

.fdyuan {
  display: flex;
  width: 100%;
  background-color: #ffffff;
  border-radius: 20rpx;
  border: 0;
  box-shadow: rgb(90 114 123 / 11%) 0px 7px 30px 0px;
}

.itimg {
  min-width: 120px;
  max-width: 120px;
  height: 90px;
  text-align: center;
  padding: 6rpx;
}

.itwz {
  width: 100%;
  text-align: center;
  padding: 12rpx 6rpx;
}

.itbt {
  font-size: 14px;
  color: rgba(0, 0, 0, 0.87);
  line-height: 1.5;
  text-align: left;
  margin-bottom: 8rpx;
  white-space: break-spaces;
}

.itms {
  text-align: left;
  font-size: x-small;
  color: rgba(0, 0, 0, 0.54);
  white-space: break-spaces;
}

.itsf {
  display: flex;
  justify-content: space-between;
  text-align: left;
  font-size: x-small;
  color: #e0620d;
  align-items: center;
  margin-top: 8rpx;
}

.itsf .nav {
  width: 32rpx;
  height: 32rpx;
  margin: 0 8rpx;
}

.img {
  width: 100%;
  height: 100%;
  border-radius: 8px;
}

.scroll-view_H {
  white-space: nowrap;
}

.scroll-view-item_H {
  display: inline-block;
  width: 80%;
  margin: 16rpx;
}

.scroll-view-item_H:first-child {
  margin-left: 16rpx;
}

.fdDH {
  position: absolute;
  z-index: 2;
  top: 24rpx;
  right: 24rpx;
  width: 60px;
  height: 460px;
  background-color: rgb(255, 255, 255);
  border-radius: 20px;
  padding: 12px 6px;
  border: 0;
  box-shadow: rgb(90 114 123 / 11%) 0px 7px 30px 0px;
}

.DHit {
  margin: auto;
  text-align: center;
  height: 30px;
  line-height: 40px;

}

.DHwz {
  font-size: 12px;
  color: rgb(39, 39, 39);
  font-weight: 600;
  text-align: center;
  height: 20px;
  line-height: 20px;
}

.DHimg {
  width: 18px;
  height: 18px;
}

.line {
  margin: auto;
  margin-top: 8px;
  width: 90%;
  height: 1rpx;
  background: rgb(177, 177, 177);
  text-align: center;

}

.itemYuan {
  display: flex;
  width: 350rpx;
  height: 60rpx;
  background-color: #ffffff;
  border-radius: 5rpx;
  opacity: 0.8;
  font-size: 12px;
  margin-top: 20px;
  position: fixed;
  text-align: center;
  left: 20%;
}

.tit_seabox {
  width: calc(100%);
  height: 60rpx;
  display: flex;
  align-items: center;
  overflow: hidden;
  margin: 24rpx;
  background-color: red;
  border-radius: 30rpx;
}

.tit_seabox_bar {
  width: calc(100%);
  display: flex;
  align-items: center;
  background-color: rgb(255, 255, 255);
  padding: 16rpx;
}

/*有权限添加活动*/
.tit_seabox_bar.tit_seabox_add {
  width: calc(100%);
}

/*开始搜索时*/
.tit_seabox_bar.tit_start_search {
  width: calc(100%);
}

/*开始搜索且有权限添加*/
.tit_seabox_bar.tit_start_search.tit_seabox_add {
  width: calc(100%);
}

.tit_seabox_bar icon {
  margin-right: 20rpx;

}

.tit_seabox input {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  width: 100%;
  margin-right: 32rpx;

}

.activity_add {
  width: 60rpx;
  text-align: right;
  border-left: 4rpx solid #f2f2f2;
  margin-left: 20rpx;

}

.activity_add image {
  width: 40rpx;
  height: 40rpx;
}

.activity_seabtn {
  font-size: 28rpx;
  width: 70rpx;
  text-align: right;
}